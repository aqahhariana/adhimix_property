<odoo>
    <data>


        <record id="view_iom_new_installment_form" model="ir.ui.view">
            <field name="name">Iom New Installment Form</field>
            <field name="model">iom.new.installment</field>
            <field name="arch" type="xml">
                <form string="Iom New Installment" version="7.0">
                    <sheet>
                        <group>
                            <field name="date_due"/>
                            <field name="unit_id"/>
                            <field name="description"/>
                            <field name="amount_total"/>
                            <field name="amount"/>
                            <field name="amount_interest"/>
                            <field name="iom_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        
        
        <record id="view_iom_new_installment_tree" model="ir.ui.view">
            <field name="name">Iom New Installment Tree</field>
            <field name="model">iom.new.installment</field>
            <field name="arch" type="xml">
                <tree string="Iom New Installment" >
                    <field name="date_due"/>
                    <field name="unit_id"/>
                    <field name="description"/>
                    <field name="amount_total"/>
                    <field name="amount"/>
                    <field name="amount_interest"/>
                    <field name="iom_id"/>
                </tree>
            </field>
        </record>


