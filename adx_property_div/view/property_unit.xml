<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>

       <record id="property_unit_rental_price_form" model="ir.ui.view">
            <field name="name">Property Unit Rental Price Form</field>
            <field name="model">property.unit.rental.price</field>
            <field name="arch" type="xml">
                <form string="Property Unit Rental Price" version="7.0">
                    <sheet>
                        <group>
                          <field name="date"/>
                          <field name="price"/>
                          <field name="pricing_type"/>
                          <field name="deskripsi"/>
                        </group>
                    </sheet>
                </form>
            </field>
      </record>
        
               
        <record id="property_unit_rental_price_tree" model="ir.ui.view">
            <field name="name">Property Unit Rental Price Tree</field>
            <field name="model">property.unit.rental.price</field>
            <field name="arch" type="xml">
                <tree string="Property Unit Rental Price">
                  <field name="date"/>
                  <field name="price"/>
                  <field name="pricing_type"/>
                  <field name="deskripsi"/>
                </tree>
            </field>
        </record>
		
		
		
		<record id="view_property_category_form" model="ir.ui.view">
            <field name="name">Property Category Form</field>
            <field name="model">property.category</field>
            <field name="arch" type="xml">
                <form string="Property Category" version="7.0">
                    <sheet>
                        <group>
                        	<group>
	                        	<field name="code" required="1"/>
	                        	<field name="name" required="1"/>
	                        	<field name="type" required="1"/>
	                        	<field name="company_id"/>
                        	</group>
                        	
                        </group>
                        <group string="Konfigurasi Akunting">
                        	<group>
	                        	<field name="property_receivable_account_id"/>
	                        	<field name="property_cogs_account_id"/>
	                        	<field name="property_account_inventory_id"/>
	                        	<field name="property_account_booking_fee_id"/>
	                        	<field name="property_account_income_id"/>
                        	</group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        
        
        <record id="view_property_category_tree" model="ir.ui.view">
            <field name="name">Property Category Tree</field>
            <field name="model">property.category</field>
            <field name="arch" type="xml">
				<tree string="Property Category" >
					<field name="code"/>
                	<field name="name"/>
                	<field name="type"/>
                	<field name="company_id"/>
				</tree>
			</field>
		</record>



		<record id="view_property_view_form" model="ir.ui.view">
