	                        	<field name="name" required="1"/>
	                        	<field name="property_category_id" required="1"/>
	                        	<field name="company_id"/>
                        	</group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="view_property_view_tree" model="ir.ui.view">
            <field name="name">Property View Tree</field>
            <field name="model">property.view</field>
            <field name="arch" type="xml">
				<tree string="Property View" >
					<field name="name"/>
                    <field name="property_category_id"/>
                    <field name="company_id"/>
