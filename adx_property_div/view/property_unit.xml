                </tree>
			</field>
		</record>
		
		


		<record id="view_property_unit_form" model="ir.ui.view">
            <field name="name">Property Unit Form</field>
            <field name="model">property.unit</field>
            <field name="arch" type="xml">
                <form string="Property Unit" version="7.0">
                    <sheet>
	                    <group>
    	                     	<field name="name" required="1"/>
    		                    <field name="address" required="1"/>
    		                    <field name="type" required="1"/>
    		                    <field name="property_category_id" required="1"/>		                   
		                        <field name="company_id"/>
                        </group>
                        <group>
                        	<group string="Status Jual">
                        		<field name="available"/>
		                        <field name="status"/>
		                        <field name="owner_id"/>
                        	</group>
                        	<group string="Status Sewa">
                        		<field name="is_rent"/>
                                <field name="rent_available"/>
                        	</group>
                        </group>
                        <group>
	                        <notebook>
		                        <page string="Informasi Lokasi dan Luas Area">
		                        	<group>
		                        		<group>
				                        	<field name="property_view_id"/>
						                    <field name="property_tower_id"/>
						                    <field name="property_type_id"/>
						                    <field name="property_jenis_id"/>
						                    <field name="property_floor_id"/>
					                    </group>
					                    <group>
					                    	<field name="direction"/>
						                    <field name="bulding_area"/>
						                    <field name="land_area"/>
						                    <field name="nett_area"/>
						                    <field name="semi_gross_area"/>
					                    </group>
				                    </group>
		                        </page>
		                        <page string="Daftar Harga Jual">
		                        	<group>
		                        		<field name="current_pricelist"/>
		                        	</group>
		                        	<field name="price_ids">
		                        		<tree string="Daftar Harga Jual" editable="Top">
		                        			<field name="date"/>
						                    <field name="name"/>
						                    <field name="net_price"/>
		                        		</tree>
		                        	</field>
		                        </page>
		                        <page string="Daftar Harga Sewa">
		                        	
		                        	<field name="rental_price_ids">
		                        		<tree string="Daftar Harga Sewa" editable="Top">
		                        			<field name="date"/>
						                    <field name="deskripsi"/>
						                    <field name="pricing_type"/>
						                    <field name="price"/>
		                        		</tree>
		                        	</field>
		                        </page>
	                        </notebook>
                        </group>
                        	
                    </sheet>
                </form>
            </field>
        </record>


        <record id="view_property_unit_tree" model="ir.ui.view">
            <field name="name">Property Unit Tree</field>
            <field name="model">property.unit</field>
            <field name="arch" type="xml">
				<tree string="Property unit" >
					<field name="name"/>
                	<field name="type"/>
                	<field name="address"/>
                	<field name="property_category_id"/>
                	<field name="property_view_id"/>
                	<field name="property_tower_id"/>
                	<field name="property_type_id"/>
                	<field name="property_floor_id"/>
                	<field name="direction"/>
                	<field name="bulding_area"/>
                	<field name="land_area"/>
                	<!-- <field name="price_ids"/> -->
                <!-- 	<field name="available"/> -->
                	<field name="status"/>
                	<field name="owner_id"/>
                	<field name="company_id"/>
                    <field name="rental_price_ids"/>
                    <field name="is_rent"/>
                    <field name="rent_available"/>
                    <field name="current_pricelist"/>
                </tree>
			</field>
		</record>
		
		<record id="view_property_price_tree" model="ir.ui.view">
            <field name="name">Property Price</field>
            <field name="model">property.unit.price</field>
            <field name="arch" type="xml">
				<tree string="Harga Jual" >
					<field name="property_unit_id"/>
                	<field name="date"/>
                	<field name="name"/>
                	<field name="net_price"/>
				</tree>
			</field>
		</record>
	
	</data>
</odoo>