		</record>
		
		
		<record id="view_property_jenis" model="ir.ui.view">
            <field name="name">Property Jenis</field>
            <field name="model">property.jenis</field>
            <field name="arch" type="xml">
				<tree string="Property type" >
					<field name="name"/>
                    <field name="property_category_id"/>
